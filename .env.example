# Databázová konfigurace pro ZDRAPP
# Zkopírujte tento soubor jako .env a upravte podle svého prostředí

# Databázové připojení
DB_HOST=localhost
DB_USERNAME=root
DB_PASSWORD=
DB_DATABASE=zdrapp
DB_PORT=3306
DB_CHARSET=utf8mb4

# Další konfigurace (pro budoucí použití)
APP_ENV=development
APP_DEBUG=true
APP_URL=http://localhost/zdrapp

# Session konfigurace
SESSION_LIFETIME=7200
SESSION_SECURE=false
SESSION_HTTPONLY=true
